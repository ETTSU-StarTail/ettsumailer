{
  "build": {
    "beforeDevCommand": "npm install && npm run dev",
    "beforeBuildCommand": "npm install && npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "ettsumailer",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.ettsu.mailer"
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "ettsumailer",
        "width": 1024,
        "height": 768
      }
    ]
  }
}
